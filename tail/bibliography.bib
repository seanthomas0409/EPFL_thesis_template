	pages = {6390--6398},
	file = {Firouzeh_Paik_2015_The Design and Modeling of a Novel Resistive Stretch Sensor With Tunable.pdf:/Users/seanthomas/Google Drive/Zotero/Firouzeh_Paik_2015/Firouzeh_Paik_2015_The Design and Modeling of a Novel Resistive Stretch Sensor With Tunable.pdf:application/pdf;IEEE Xplore Abstract Record:/Users/seanthomas/Zotero/storage/3GQ6ZIRI/7164275.html:text/html},
