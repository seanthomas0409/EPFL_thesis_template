	journal = {Advanced Engineering Materials},
	author = {Must, Indrek and Kaasik, Friedrich and Põldsalu, Inga and Mihkels, Lauri and Johanson, Urmas and Punning, Andres and Aabloo, Alvo},
	year = {2015},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/adem.201400246},
	pages = {84--94},
	file = {Full Text PDF:/Users/seanthomas/Zotero/storage/K2KYXEBB/Must et al. - 2015 - Ionic and Capacitive Artificial Muscle for Biomime.pdf:application/pdf;Snapshot:/Users/seanthomas/Zotero/storage/IUCFJHHT/adem.html:text/html},
