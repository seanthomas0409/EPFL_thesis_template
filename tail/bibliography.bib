	keywords = {SMA actuator, Capture range, Gripping force, Large-stroke, Parallel gripper},
	pages = {74--80},
	file = {ScienceDirect Full Text PDF:/Users/seanthomas/Zotero/storage/K6GL4W25/Lu et al. - 2019 - A novel design of a parallel gripper actuated by a.pdf:application/pdf;ScienceDirect Snapshot:/Users/seanthomas/Zotero/storage/847JW6G2/S0020740319311440.html:text/html},
}

@inproceedings{motzkiEnergyefficientSMAVacuum2016,
