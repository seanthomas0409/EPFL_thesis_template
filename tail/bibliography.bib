	note = {Publisher: SAGE Publications Ltd STM},
	keywords = {antagonistic configuration, bionic design, differential friction, shape memory alloy, soft robots},
	pages = {1920--1934},
	file = {SAGE PDF Full Text:/Users/seanthomas/Zotero/storage/JPI4CBDE/Liang et al. - 2020 - A shape memory alloy–actuated soft crawling robot .pdf:application/pdf},
}

@article{dongDesignExperimentPiezoelectric2020,
