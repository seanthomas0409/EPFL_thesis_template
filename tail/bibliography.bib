	journal = {Extreme Mechanics Letters},
	author = {Cao, Jiawei and Qin, Lei and Liu, Jun and Ren, Qinyuan and Foo, Choon Chiang and Wang, Hongqiang and Lee, Heow Pueh and Zhu, Jian},
