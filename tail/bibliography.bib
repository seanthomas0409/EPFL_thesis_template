	year = {2016},
	file = {Full Text PDF:/Users/seanthomas/Zotero/storage/KNZIADCL/Motzki et al. - 2016 - Energy-efficient SMA Vacuum Gripper System.pdf:application/pdf},
}

@article{rodrigueCurvedShapeMemory2017a,
