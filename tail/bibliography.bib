	urldate = {2021-10-26},
	journal = {Additive Manufacturing},
	author = {Zhu, Jia-Ning and Borisov, Evgenii and Liang, Xiaohui and Farber, Eduard and Hermans, M. J. M. and Popovich, V. A.},
	month = feb,
